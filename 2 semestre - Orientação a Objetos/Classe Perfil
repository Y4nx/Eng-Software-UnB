public abstract class Perfil {
	protected String email;
	protected String pais;
	protected String cidade;
	protected String fusoHorario;
	protected String emailOpcional;
	protected String genero;
	
	public Perfil(String email, String pais, String cidade, String fusoHorario, String emailOpcional, String genero) {
		this.email = email;
		this.pais = pais;
		this.cidade = cidade;
		this.fusoHorario = fusoHorario;
		this.emailOpcional = emailOpcional;
		this.genero = genero;
	}
	
	public Perfil() {
		
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public void setPais(String pais) {
		this.pais = pais;
	}

	public void setCidade(String cidade) {
		this.cidade = cidade;
	}

	public void setFusoHorario(String fusoHorario) {
		this.fusoHorario = fusoHorario;
	}

	public void setEmailOpcional(String emailOpcional) {
		this.emailOpcional = emailOpcional;
	}

	public void setGenero(String genero) {
		this.genero = genero;
	}
}
